{
  "name": "jTools",
  "version": "0.1",
  "description": "Tools for manipulating and analysing web pages using jQuery.",
  "browser_action": {
    "default_icon": "images/jquery.png",
	"popup": "popup.html"
  },
	"permissions": [
		"tabs"
	],
	 "content_scripts": 
	[{
	 "matches": ["<all_urls>"],
	 "css": ["elements.css"],
	 "js": ["jquery-1.5.2.min.js","jquery.getPath.js","jquery.getPathObject.js","pathContainer.js","pathObject.js","jtools_interface.js","controller.js","main.js"]
	}]
}